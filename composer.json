{
    "name": "blackforest/piwik-bundle",
    "description": "Contao piwik bundle",
    "keywords": [
        "contao",
        "bundle"
    ],
    "type": "bundle",
    "homepage": "https://github.com/BlackForest",
    "license": "LGPL-3.0-or-later",
    "authors": [
